// 13.  Listar las reservas pendientes de un cliente especifico
use("db_campus_alquiler");
db.getCollection("Reserva").aggregate([
    {
        $match: {
          Cliente_ID:5,
          Estado:"Pendiente"
        }
    },
    {
        $lookup: {
          from: "Cliente",
          localField: "Cliente_ID",
          foreignField: "ID",
          as: "Cliente_Info"
        }
    },
    {
        $unwind: "$Cliente_Info"
    },
    {
        $lookup: {
          from: "Automovil",
          localField: "Automovil_ID",
          foreignField: "ID",
          as: "Auto_Info"
        }
    },
    {
        $unwind: "$Auto_Info"
    },
    {
        $project: {
          _id:0,
          "Cliente_Info._id":0,
          "Auto_Info._id":0
        }
    },
    {
        $group: {
          _id: "$ID",
          fecha_inicio: { $first: "$Fecha_Inicio" },
          fecha_fin: { $first: "$Fecha_Fin" },
          estado: { $first: "$Estado" },
          cliente_info: { $push: "$Cliente_Info" },
          automovil_info: { $push: "$Auto_Info" },
        }
    }
]);